{
  "name": "YouLikedIt",
  "description": "Automatically LIKE every video you watch.",
  "version": "0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.youtube.com/watch?v=*/"
  ],
  "action": {
    "default_icon": "you_liked_it.png"
  }
}